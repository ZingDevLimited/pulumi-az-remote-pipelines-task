{
    "manifestVersion": 1,
    "id": "pulumi-az-remote-pipelines-task",
    "name": "Pulumi Azure Remote Pipelines Task",
    "version": "0.0.51",
    "publisher": "danhyrjak",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "description": "Azure Pipelines Build/Release task for running Pulumi apps where state is managed remotely via blob storage and secrets are encrypted via a auto generated passphrase stored in key vault.",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "images/extension-icon.png"
    },
    "files": [
        {
            "path": "buildAndReleaseTask"
        }
    ],
    "contributions": [
        {
            "id": "pulumi-az-remote-pipelines-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "buildAndReleaseTask"
            }
        }
    ]
}