{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "eofline": false,
        "no-console": false,
        "ordered-imports": true,
        "no-string-literal": false,
        "no-floating-promises": [
            true,
            "Q.Promise"
        ],
        "object-literal-key-quotes": true,
        "quotemark": [
            false
        ]
    },
    "rulesDirectory": [],
    "linterOptions": {
        "exclude": [
            "tests/**/*.ts"
        ]
    }
}