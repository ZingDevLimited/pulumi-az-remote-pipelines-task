{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "comment-format": false,
        "eofline": false,
        "max-classes-per-file": false,
        "max-line-length": [
            true,
            150
        ],
        "no-console": false,
        "no-floating-promises": [
            true,
            "Q.Promise"
        ],
        "no-string-literal": false,
        "object-literal-key-quotes": [
            true,
            "consistent-as-needed"
        ],
        "one-line": [
            true,
            "check-open-brace",
            "check-whitespace"
        ],
        "ordered-imports": false,
        "quotemark": [
            false
        ]
    },
    "rulesDirectory": [],
    "linterOptions": {
        "exclude": [
            "tests/**/*.ts"
        ]
    }
}